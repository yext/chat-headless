{
  "name": "@yext/chat-headless-react",
  "version": "0.1.0",
  "description": "A library for powering UI components for Yext Chat integrations",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "keywords": [
    "chat",
    "yext",
    "redux",
    "state management"
  ],
  "files": [
    "dist",
    "src",
    "THIRD-PARTY-NOTICES"
  ],
  "scripts": {
    "test": "jest --config=jest.config.json",
    "lint": "eslint . --max-warnings=0",
    "tsc-cjs": "tsc -p tsconfig.cjs.json",
    "tsc-esm": "tsc -p tsconfig.esm.json",
    "dev": "npm run tsc-esm -- --watch",
    "build": "rm -rf dist/** && npm run tsc-esm && npm run tsc-cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yext/chat-headless.git"
  },
  "author": "clippy@yext.com",
  "bugs": {
    "url": "https://github.com/yext/chat-headless/issues"
  },
  "homepage": "https://github.com/yext/chat-headless#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@yext/chat-headless": "*",
    "react": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@microsoft/api-documenter": "^7.22.4",
    "@microsoft/api-extractor": "^7.34.8",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.5",
    "@yext/eslint-config-slapshot": "^0.7.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "typescript": "^5.0.4"
  }
}
